<!DOCTYPE html>
<html lang="pt-BR">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Table</title>
    <style>
        table{
            border: 1px black solid;

        }
        table th{
            border: 1px black solid;
            padding: 1rem;
        }
        table td{
            border: 1px black solid;
            padding: 1rem;
        }
    </style>
    
</head>

<body>

    <table cellspacing = '0'>
        <thead>
            <% header.forEach(col => { %>
            <th><%= col %></th>
            <% }) %>
        </thead>
        <tbody>
            <% rows.forEach(row => { %>
            <tr>
                <% row.forEach(element => { %>
                <td> <%= element %> </td>
                <% }) %>
            </tr>
            <% }) %>
        </tbody>

    </table>


</body>

</html>